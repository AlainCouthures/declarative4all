    <dependency type="spec" value="XQ10+"/>
    <test>declare function local:dayTimeDuration($dayTimeDuration as xs:dayTimeDuration, $null as xs:boolean) { if ($null) then () else $dayTimeDuration }; exists(local:dayTimeDuration(xs:dayTimeDuration(&quot;P0D&quot;), fn:true()) lt xs:dayTimeDuration(&quot;P0D&quot;))</test>
