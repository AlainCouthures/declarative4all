            <created by="Carmelo Montanez" on="2005-03-08"/>
        </schema>        
        <source role="." file="docs/atomic.xml" validation="strict" uri="http://www.w3.org/fots/docs/atomic.xml">
            <description>A Schema validated xml file, that contains values for data types. Can be used by any test.</description>
            <created by="Carmelo Montanez" on="2005-03-08"/>
        </source>
   </environment>
   
