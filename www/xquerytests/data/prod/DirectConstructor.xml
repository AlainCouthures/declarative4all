<test-set name="prod-DirectConstructor" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">
  <description>Tests for the DirectConstructor production</description>
  <link document="http://www.w3.org/TR/xquery-30/" idref="doc-xquery30-DirectConstructor" type="spec"/>
  <link document="XQuery" idref="id-otherConstructors" section-number="3.7.2" type="spec"/>
  <test-case name="Constr-pi-content-1">
    <description> PI constructor - regular </description>
    <created by="Andreas Behm" on="2005-02-22"/>
    <dependency type="spec" value="XQ10+"/>
    <test>
      <![CDATA[<?pi content?>]]>
    </test>
    <result-string/>
    <result>
      <assert-xml>
        <![CDATA[<?pi content?>]]>
      </assert-xml>
    </result>
  </test-case>
