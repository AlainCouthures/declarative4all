<?xml-stylesheet href="xsl/xsltforms.xsl" type="text/xsl"?>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xf="http://www.w3.org/2002/xforms" lang="en">
    <head>
        <meta name="viewport" content="initial-scale=1, width=device-width, viewport-fit=cover"/>
        <title>Books</title>
        <model xmlns="http://www.w3.org/2002/xforms">
            <instance>
                <data xmlns="">
                    <book year="2013" title="XForms with XSLTForms"/>
                </data>
            </instance>
        </model>
    </head>
    <body>
		<xf:repeat nodeset="book">
			<xf:output value="@year"/> - <xf:output value="@title"/>
		</xf:repeat>
    </body>
</html>