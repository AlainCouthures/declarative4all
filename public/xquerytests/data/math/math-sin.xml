<test-set covers="math-sin" name="math-sin" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">
  <description>Tests for the math:sin function introduced in XPath 3.0</description>
  <link document="http://www.w3.org/TR/xpath-functions-30/" idref="func-sin" type="spec"/>
  <dependency type="spec" value="XP30+ XQ30+"/>
  <test-case name="math-sin-001"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">()</response-string>
    <description>Evaluate the function math:sin() with the argument ()</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(())</test>
    <result-string>()</result-string>
    <result>
      <assert-empty/>
    </result>
  </test-case>
  <test-case name="math-sin-002"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("0")</response-string>
    <description>Evaluate the function math:sin() with the argument 0</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(0)</test>
    <result-string>xs:double("0")</result-string>
    <result>
      <assert-eq>0.0e0</assert-eq>
    </result>
  </test-case>
  <test-case name="math-sin-003"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("0")</response-string>
    <description>Evaluate the function math:sin() with the argument -0.0e0</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(-0.0e0)</test>
    <result-string>xs:double("0")</result-string>
    <result>
      <assert-eq>-0.0e0</assert-eq>
    </result>
  </test-case>
  <test-case name="math-sin-004"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("1")</response-string>
    <description>Evaluate the function math:sin() with the argument math:pi() div 2</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(math:pi() div 2)</test>
    <result-string>xs:double("1")</result-string>
    <result>
      <assert-eq>1.0e0</assert-eq>
    </result>
  </test-case>
  <test-case name="math-sin-005"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("-1")</response-string>
    <description>Evaluate the function math:sin() with the argument -math:pi() div 2</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(-math:pi() div 2)</test>
    <result-string>xs:double("-1")</result-string>
    <result>
      <assert-eq>-1.0e0</assert-eq>
    </result>
  </test-case>
  <test-case name="math-sin-006"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:boolean("true")</response-string>
    <description>Evaluate the function math:sin() with the argument math:pi()</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(math:pi()) lt 1e-15</test>
    <result-string>xs:boolean("true")</result-string>
    <result>
      <assert-true/>
    </result>
  </test-case>
  <test-case name="math-sin-007"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("NaN")</response-string>
    <description>Evaluate the function math:sin() with the argument xs:double(&apos;NaN&apos;)</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(xs:double(&apos;NaN&apos;))</test>
    <result-string>xs:double("NaN")</result-string>
    <result>
      <assert-string-value>NaN</assert-string-value>
    </result>
  </test-case>
  <test-case name="math-sin-008"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("NaN")</response-string>
    <description>Evaluate the function math:sin() with the argument xs:double(&apos;INF&apos;)</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(xs:double(&apos;INF&apos;))</test>
    <result-string>xs:double("NaN")</result-string>
    <result>
      <assert-string-value>NaN</assert-string-value>
    </result>
  </test-case>
  <test-case name="math-sin-009"><response-string xmlns:qt="http://www.w3.org/2010/09/qt-fots-catalog" xmlns="http://www.w3.org/2010/09/qt-fots-catalog">xs:double("NaN")</response-string>
    <description>Evaluate the function math:sin() with the argument xs:double(&apos;-INF&apos;)</description>
    <created by="O&apos;Neil Delpratt, Saxonica" on="2010-12-10"/>
    <environment ref="math"/>
    <test>math:sin(xs:double(&apos;-INF&apos;))</test>
    <result-string>xs:double("NaN")</result-string>
    <result>
      <assert-string-value>NaN</assert-string-value>
    </result>
  </test-case>
</test-set>